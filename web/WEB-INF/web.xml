<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ExamSearch</servlet-name>
        <servlet-class>ExamSearch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AssignmentSearch</servlet-name>
        <servlet-class>AssignmentSearch</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ExamSearch</servlet-name>
        <url-pattern>/ExamSearch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AssignmentSearch</servlet-name>
        <url-pattern>/AssignmentSearch</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>exam-search.jsp</welcome-file>
    </welcome-file-list>
     <welcome-file-list>
        <welcome-file>mainUI.html</welcome-file>
    </welcome-file-list>
</web-app>